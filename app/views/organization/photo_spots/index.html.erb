<%= render 'shared/breadcrumbs_mypage', { title: "登録したフォトスポット" } %>

<div class="row">

  <!-- Listings -->
  <div class="col-lg-12 col-md-12">
    <div class="dashboard-list-box margin-top-0">
      <% if @photo_spots.size == 0 %>
        <h4>登録したフォトスポットはありません</h4>
      <% else %>
        <ul>
          <%= render @photo_spots %>
        </ul>
      <% end %>
    </div>

    <div class="clearfix"></div>
    <div class="pagination-container margin-top-30 margin-bottom-0">
      <%= paginate @photo_spots %>
    </div>

  </div>
